alias l='ls -lartp'
alias la='ls -la'
alias lg='ls -la | grep'
alias hg='history | grep'

alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

alias digitalocean='ssh digitalocean'
alias www='cd ~/Devel/server/public'

alias upgrade='sudo apt -y update && sudo apt -y full-upgrade'
alias autoremove='sudo apt autoremove && sudo apt autoclean'

