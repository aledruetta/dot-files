alias l='ls -lartp'
alias la='ls -la'
alias lg='ls -la | grep'
alias hg='history | grep'

alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

alias tmux="TERM=screen-256color-bce tmux"
alias docean="ssh digitalocean"
alias upgrade="sudo apt -y update && sudo apt -y full-upgrade"
alias ffile="find . -type f -name"
